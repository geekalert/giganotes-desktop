{
  "name": "giganotes",
  "version": "1.1.6",
  "description": "Note taking application",
  "homepage": "https://giganotes.com",
  "author": {
    "name": "FourthByte",
    "email": "4thbyte@gmail.com"
  },
  "keywords": [
    "giganites",
    "note-taking"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps && npm run rebuild-core",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e",
    "rebuild-core": "electron-build-env neon build giganotes-core-js --release"
  },
  "dependencies": {
    "@angular/animations": "9.1.4",
    "@angular/cdk": "9.2.2",
    "@angular/common": "9.1.4",
    "@angular/compiler": "9.1.4",
    "@angular/core": "9.1.4",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.1.4",
    "@angular/http": "7.2.16",
    "@angular/material": "9.2.2",
    "@angular/router": "9.1.4",
    "@auth0/angular-jwt": "4.0.0",
    "@tinymce/tinymce-angular": "2.0.1",
    "angular-draggable-droppable": "4.4.4",
    "async-file": "2.0.2",
    "electron-oauth-helper": "3.0.0",
    "giganotes-core-js": "file:../giganotes-core-js",
    "material-design-icons": "^3.0.1",
    "node-abi": "2.16.0",
    "roboto-fontface": "^0.10.0",
    "rxjs": "6.5.5",
    "uuid": "8.0.0"
  },
  "devDependencies": {
    "electron-build-env": "^0.2.0",
    "@angular-builders/custom-webpack": "9.1.0",
    "@angular-devkit/build-angular": "0.901.4",
    "@angular/cli": "9.1.4",
    "@angular/compiler-cli": "9.1.4",
    "@angular/language-service": "9.1.4",
    "@angular/platform-browser": "9.1.4",
    "@angular/platform-browser-dynamic": "9.1.4",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.11.1",
    "codelyzer": "5.2.2",
    "core-js": "3.6.5",
    "electron": "8.2.5",
    "electron-builder": "22.6.0",
    "electron-rebuild": "1.10.1",
    "electron-reload": "1.5.0",
    "hammerjs": "^2.0.8",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.0.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.0",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "npm-run-all": "4.1.5",
    "npx": "10.2.2",
    "protractor": "5.4.4",
    "ts-node": "8.10.1",
    "tslint": "6.1.2",
    "typescript": "3.8.3",
    "wait-on": "4.0.2",
    "webdriver-manager": "12.1.7",
    "zone.js": "0.10.3",
    "webpack": "4.43.0"
  }
}
